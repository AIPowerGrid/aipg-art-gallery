# Supabase Configuration (Optional - needed for community gallery feature)
# Create a free project at https://supabase.com/ and run the migration in supabase/init.sql
# Leave these empty or as placeholders to disable the gallery feature
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Gallery API Configuration
NEXT_PUBLIC_GALLERY_API=http://localhost:4000/api

# === Server Configuration (Go backend) ===

# Server address
GALLERY_SERVER_ADDR=:4000

# AIPG API settings (Grid API for job submission)
AIPG_API_URL=https://api.aipowergrid.io/api/v2
AIPG_CLIENT_AGENT=AIPG-Art-Gallery:v2
AIPG_API_KEY=CADWOATvdPmE7npsuLv1bQ

# Path to model presets JSON file
MODEL_PRESETS_PATH=./server/config/model_presets.json

# CORS allowed origins (comma-separated)
GALLERY_ALLOWED_ORIGINS=http://localhost:3000

# === ModelVault Blockchain Configuration ===
# Models are sourced from the ModelVault contract on Base Mainnet
# This is the same source of truth used by comfy-bridge workers

# Enable/disable blockchain model registry (default: true)
MODELVAULT_ENABLED=true

# Base Mainnet RPC URL
MODELVAULT_RPC_URL=https://mainnet.base.org

# ModelVault contract address on Base Mainnet  
MODELVAULT_CONTRACT=0x79F39f2a0eA476f53994812e6a8f3C8CFe08c609

# === RecipeVault Blockchain Configuration ===
# Recipes/workflows are sourced from the RecipeVault facet (same diamond proxy as ModelVault)
# Available models are extracted from recipes in RecipeVault

# Enable/disable blockchain recipe registry (default: true)
RECIPESVAULT_ENABLED=true

# Base Mainnet RPC URL (defaults to MODELVAULT_RPC_URL if not set)
RECIPESVAULT_RPC_URL=https://mainnet.base.org

# RecipeVault contract address (defaults to MODELVAULT_CONTRACT - same diamond proxy)
RECIPESVAULT_CONTRACT=0x79F39f2a0eA476f53994812e6a8f3C8CFe08c609

# === Wallet Connect Configuration ===
# Get your project ID from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=